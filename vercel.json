{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/reactapp"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "backend-pi-rouge.vercel.app/api",
    "REACT_APP_API_UPLOADS": "backend-pi-rouge.vercel.app/uploads"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
